--langdef=js
--langmap=js:.js
--regex-js=/([A-Za-z0-9_]+)[ \t]*[:=][ \t]*\{/\1/,object/
--regex-js=/([A-Za-z0-9_]+)[ \t]*[:=][ \t]*function[ \t]*\(/\1/,function/
--regex-js=/function[ \t]+([A-Za-z0-9_]+)[ \t]*\(([^)])\)/\1/,function/
--langdef=cheetah
--langmap=cheetah:.tmpl
--regex-cheetah=/#def[ \t]+([A-Za-z0-9_]+)/\1/,function/
--langdef=actionscript
--langmap=actionscript:.as
--regex-actionscript=/^[ \t]*[(override|protected|private|public|static) ( \t)]*function[ \t]+([A-Za-z0-9_]+)[ \t]*\(/\1/f, function, functions/
--regex-actionscript=/^[ \t]*[(public) ( \t)]*function[ \t]+(set|get) [ \t]+([A-Za-z0-9_]+)[ \t]*\(/\1 \2/p,property, properties/
--regex-actionscript=/^[ \t]*[(private|public|static|protected) ( \t)]*var[ \t]+([A-Za-z0-9_]+)[ \t]*/\1/v,variable, variables/
--regex-actionscript=/.*\.prototype \.([A-Za-z0-9 ]+)=([ \t]?)function( [ \t]?)*\(/\1/ f,function, functions/
--regex-actionscript=/^[ \t]*class[ \t]+([A-Za-z0-9_]+)[ \t]*/\1/c,class, classes/
